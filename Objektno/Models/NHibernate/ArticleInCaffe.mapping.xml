<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
assembly="Objektno"
namespace="Objektno.Models">
  <class name="ArticleInCaffeModel" table="ArticleInCaffe" batch-size="25">
    <id name="ID">
      <generator class="increment" />
    </id>
    <property name="IDArticle" not-null="true" />
    <property name="IDCaffe" not-null="true" />
    <property name="IsAvailable" not-null="true" />

    <many-to-one name="Caffe" class="CaffeModel" column="IDCaffe" />
    <many-to-one name="Article" class="ArticleModel" column="IDArticle" />
  </class>
</hibernate-mapping>