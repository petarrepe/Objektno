<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
assembly="DAL"
namespace="DAL.Models">
  <class name="ArticleInCaffe" table="ArticleInCaffe" batch-size="25">
    <id name="ID">
      <generator class="native" />
    </id>
    <property name="IDArticle" not-null="true" />
    <property name="IDCaffe" not-null="true" />
    <property name="IsAvailable" not-null="true" />

    <many-to-one name="Caffe" class="Caffe" column="IDCaffe" />
    <many-to-one name="Article" class="Article" column="IDArticle" />
  </class>
</hibernate-mapping>